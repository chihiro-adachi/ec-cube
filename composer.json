{
    "name": "ec-cube/ec-cube",
    "description": "EC-CUBE EC open platform.",
    "type": "project",
    "homepage": "http://www.ec-cube.net/",
    "license": [
       "GPL-2.0",
       "Commercial"
    ],
    "support": {
        "issues": "https://github.com/EC-CUBE/ec-cube/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^5.6 || ^7.0",
        "silex/silex": "*",
        "silex/web-profiler": "*",
        "doctrine/orm": "*",
        "symfony/monolog-bridge": "3.3.*",
        "symfony/twig-bridge": "3.3.*",
        "symfony/finder": "3.3.*",
        "symfony/yaml": "3.3.*",
        "symfony/form": "3.3.*",
        "symfony/validator": "3.3.*",
        "symfony/translation": "3.3.*",
        "symfony/config": "3.3.*",
        "symfony/security": "3.3.*",
        "symfony/doctrine-bridge": "3.3.*",
        "symfony/class-loader": "3.3.*",
        "symfony/css-selector": "3.3.*",
        "symfony/dom-crawler": "3.3.*",
        "symfony/serializer": "3.3.*",
        "dflydev/doctrine-orm-service-provider": "*",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "*",
        "saxulum/saxulum-webprofiler-provider": "*",
        "swiftmailer/swiftmailer": "5.*",
        "knplabs/knp-components": "*",
        "doctrine/migrations": "*",
        "knplabs/console-service-provider": "*",
        "nesbot/carbon": "*",
        "egulias/email-validator": "^1.2",
        "symfony/console": "3.3.*",
        "symfony/debug": "3.3.*",
        "symfony/filesystem": "3.3.*",
        "symfony/http-foundation": "3.3.*",
        "symfony/http-kernel": "3.3.*",
        "symfony/intl": "3.3.*",
        "symfony/options-resolver": "3.3.*",
        "symfony/process": "3.3.*",
        "symfony/property-access": "3.3.*",
        "symfony/routing": "3.3.*",
        "symfony/stopwatch": "3.3.*",
        "symfony/web-profiler-bundle": "3.3.*",
        "symfony/var-dumper": "3.3.*",
        "symfony/debug-bundle": "3.3.*",
        "symfony/inflector": "3.3.*",
        "psr/log": "^1.0",
        "composer/ca-bundle": "^1.0",
        "ext-mbstring": "*",
        "sergiors/annotations-service-provider": "dev-master",
        "sergiors/routing-service-provider": "dev-master",
        "sergiors/templating-service-provider": "^2",
        "sergiors/sensio-framework-extra-service-provider": "dev-master",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/event-dispatcher": "3.3.*",
        "symfony/dotenv": "3.3.*",
        "mobiledetect/mobiledetectlib": "~2",
        "doctrine/data-fixtures": "~1.1",
        "saxulum/saxulum-validator-provider": "^2.1",
        "hirak/prestissimo": "^0.3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mikey179/vfsStream": "~1",
        "fzaninotto/faker":"1.5.*",
        "phing/phing": "2.*",
        "symfony/browser-kit": "3.3.*",
        "friendsofphp/php-cs-fixer": "^2.2",
        "satooshi/php-coveralls": "2.0.x-dev"
    },
    "autoload": {
        "files": [ "src/Eccube/Resource/functions/log.php" ],
        "psr-4": {
            "Eccube\\Entity\\": "app/proxy/entity",
            "Eccube\\": "src/Eccube",
            "Acme\\": "app/Acme",
            "Plugin\\": "app/Plugin",
            "Dbtlr\\MigrationProvider\\Provider\\": "src/silex-doctrine-migrations"
        },
        "exclude-from-classmap": ["**/Test/**", "**/Tests/**", "**/test/**", "**/tests/**"]
    },
    "autoload-dev" : {
        "psr-4": {
            "Eccube\\Tests\\" : "tests/Eccube/Tests"
        }
    },
    "scripts": {
        "compile": [
            "php eccube_install.php pgsql none --skip-createdb -V"
        ]
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        },
        "preferred-install": {
            "*": "dist"
        }
    }
}
